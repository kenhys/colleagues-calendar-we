{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": "background/index.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/*"
      ],
      "js": "content_scripts/index.js"
    }
  ],
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "browsingData",
    "contextMenus",
    "contextualIdentities",
    "cookies",
    "downloads",
    "downloads.open",
    "history",
    "identity",
    "idle",
    "management",
    "nativeMessaging",
    "notifications",
    "sessions",
    "storage",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ]
}
